{
    "name": "ignitor/queues",
    "description": "Better Queues for CodeIgniter 4",
    "type": "library",
    "keywords": ["framework", "CodeIgniter", "Queues"],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Igniter\\Queues\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests"
        }
    },
    "authors": [
        {
            "name": "Hamid Ssemitala",
            "email": "semix.hamidouh@gmail.com"
        }
    ],
    "require": {
        "php": ">= 8.1",
        "opis/closure": "^3.6"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5|^8.0",
        "codeigniter4/framework": "^4.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        
    },
    "config": {
        "sort-packages": true
    }
}
